{
  "$defs": {
    "ApiConfig": {
      "properties": {
        "protocol": {
          "description": "API protocol (e.g., rest)",
          "title": "Protocol",
          "type": "string"
        },
        "protocol_format": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Format style (e.g., openai, anthropic)",
          "title": "Protocol Format"
        },
        "base_url": {
          "description": "Base URL for the API",
          "title": "Base Url",
          "type": "string"
        },
        "auth": {
          "$ref": "#/$defs/AuthConfig",
          "description": "Authentication configuration"
        },
        "endpoints": {
          "anyOf": [
            {
              "additionalProperties": {
                "$ref": "#/$defs/EndpointConfig"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Key endpoints definition",
          "title": "Endpoints"
        },
        "error_codes": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/ErrorCode"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Common error codes",
          "title": "Error Codes"
        }
      },
      "required": [
        "protocol",
        "base_url",
        "auth"
      ],
      "title": "ApiConfig",
      "type": "object"
    },
    "AuthConfig": {
      "properties": {
        "type": {
          "description": "Authentication type (e.g., api_key)",
          "title": "Type",
          "type": "string"
        },
        "header_key": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Header key for the token",
          "title": "Header Key"
        },
        "token_prefix": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Prefix for the token (e.g., Bearer)",
          "title": "Token Prefix"
        },
        "env_var_suggestion": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Suggested environment variable name",
          "title": "Env Var Suggestion"
        }
      },
      "required": [
        "type"
      ],
      "title": "AuthConfig",
      "type": "object"
    },
    "EndpointConfig": {
      "properties": {
        "path": {
          "description": "API endpoint path",
          "title": "Path",
          "type": "string"
        },
        "method": {
          "description": "HTTP method",
          "title": "Method",
          "type": "string"
        },
        "supports_stream": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "Whether streaming is supported",
          "title": "Supports Stream"
        }
      },
      "required": [
        "path",
        "method"
      ],
      "title": "EndpointConfig",
      "type": "object"
    },
    "ErrorCode": {
      "properties": {
        "code": {
          "description": "HTTP status code or error code",
          "title": "Code",
          "type": "string"
        },
        "description": {
          "description": "Description of the error",
          "title": "Description",
          "type": "string"
        }
      },
      "required": [
        "code",
        "description"
      ],
      "title": "ErrorCode",
      "type": "object"
    },
    "Model": {
      "properties": {
        "id": {
          "description": "Unique model identifier",
          "title": "Id",
          "type": "string"
        },
        "name": {
          "description": "Display name of the model",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "description": "Model type (chat, embedding, video, etc.)",
          "title": "Type",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Brief description of the model",
          "title": "Description"
        },
        "features": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "List of supported features",
          "title": "Features"
        },
        "context_window": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum context window size",
          "title": "Context Window"
        },
        "max_output_tokens": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum output tokens",
          "title": "Max Output Tokens"
        },
        "pricing": {
          "anyOf": [
            {
              "$ref": "#/$defs/Pricing"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Pricing information"
        }
      },
      "required": [
        "id",
        "name",
        "type"
      ],
      "title": "Model",
      "type": "object"
    },
    "Pricing": {
      "properties": {
        "input": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Cost per 1M input tokens",
          "title": "Input"
        },
        "output": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Cost per 1M output tokens",
          "title": "Output"
        }
      },
      "title": "Pricing",
      "type": "object"
    },
    "Provider": {
      "properties": {
        "id": {
          "description": "Unique provider identifier",
          "title": "Id",
          "type": "string"
        },
        "name": {
          "description": "Display name of the provider",
          "title": "Name",
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Description of the provider",
          "title": "Description"
        },
        "website": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Provider website URL",
          "title": "Website"
        },
        "docs_url": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Documentation URL",
          "title": "Docs Url"
        },
        "api_config": {
          "$ref": "#/$defs/ApiConfig",
          "description": "API connection configuration"
        },
        "pricing_currency": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "USD",
          "description": "Currency for pricing",
          "title": "Pricing Currency"
        },
        "models": {
          "description": "List of supported models",
          "items": {
            "$ref": "#/$defs/Model"
          },
          "title": "Models",
          "type": "array"
        }
      },
      "required": [
        "id",
        "name",
        "api_config",
        "models"
      ],
      "title": "Provider",
      "type": "object"
    }
  },
  "properties": {
    "$schema": {
      "description": "Path to the JSON schema",
      "title": "$Schema",
      "type": "string"
    },
    "version": {
      "description": "Registry version",
      "title": "Version",
      "type": "string"
    },
    "updated_at": {
      "description": "Last update date (YYYY-MM-DD)",
      "title": "Updated At",
      "type": "string"
    },
    "description": {
      "description": "Registry description",
      "title": "Description",
      "type": "string"
    },
    "providers": {
      "description": "List of LLM providers",
      "items": {
        "$ref": "#/$defs/Provider"
      },
      "title": "Providers",
      "type": "array"
    }
  },
  "required": [
    "$schema",
    "version",
    "updated_at",
    "description",
    "providers"
  ],
  "title": "LLMRegistry",
  "type": "object"
}